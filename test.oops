class FibIterator uses Iterator
	def init(c, n)
		self.c = c
		self.n = n
	end
	def get()
		self.c
	end
	def next()
		FibIterator(self.n, self.c + self.n)
	end
	def done()
		self.c > 4000000
	end
end

print(FibIterator(1, 1).filter(fun(x) x % 2 == 0 end).sum())

class Cons
	def init(h, t)
		self.h = h
		self.t = t
	end
end

class Nil end

def traverse(list)
	match list
		case Cons(h, t) then
			print(h)
			traverse(t)
		case Nil() then
			print("nil")
	end
end

traverse(Cons(1, Cons(2, Cons(3, Cons(4, Nil())))))