print("Welcome to oops")

trait Iterator
	get
	next
	done
end

class Range uses Iterator
	def init(start, stop, step)
		self.start = start
		self.stop = stop
		self.step = step
	end
	def get()
		self.start
	end
	def next()
		Range(self.start + self.step, self.stop, self.step)
	end
	def done()
		self.start > self.stop
	end
end